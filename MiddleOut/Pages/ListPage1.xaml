<UserControl x:Class="MiddleOut.ListPage1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             >
    <Grid Style="{StaticResource ContentRoot}">
        <!-- TODO: set @SelectedSource -->
            <mui:ModernTab x:Name="ListPageHome" Layout="List" >
            <mui:ModernTab.Links>
                <!-- TODO: set @Source -->
                <mui:Link DisplayName="Home" Source="Pages/Home.xaml"/>
                <mui:Link DisplayName="Add A Service" Source="/ChooseServicePage.xaml" />
                <mui:Link DisplayName="Mapping" Source="/MapPage.xaml" />
                <mui:Link DisplayName="Notifications" Source="/NotificationPage.xaml" />
                <mui:Link DisplayName="About" Source="/AboutPage.xaml" />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
